	$("input[name=meat]").change(function(){
    var max= 2;
    if( $("input[name=meat]:checked").length == max ){
        $("input[name=meat]").attr('disabled', 'disabled');
        $("input[name=meat]:checked").removeAttr('disabled');
    }else{
         $("input[name=meat]").removeAttr('disabled');
    }
});

